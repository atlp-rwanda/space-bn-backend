{
  "name": "clear",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/atlp-rwanda/space-bn-backend.svg?branch=develop)](https://travis-ci.org/atlp-rwanda/space-bn-backend)",
  "main": "index.js",
  "scripts": {
    "test": "set NODE_ENV=test && nyc --reporter=html --reporter=text mocha --require @babel/register ./src/tests/*.test.js --recursive ./src/tests/*.test.js --exit",
    "lint": "eslint ./",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "coverage": "nyc reporter --reporter=text --lcov | coveralls",
    "start:dev": "nodemon --exec babel-node src/index.js",
    "start": "nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "createTables": "npx sequelize db:migrate",
    "dropTables": "npx sequelize db:migrate:undo:all",
    "seeders": "npx sequelize-cli db:seed:all",
    "migration": "babel-node node_modules/.bin/sequelize db:migrate",
    "migration:seed": "babel-node node_modules/.bin/sequelize db:seed:all",
    "migration:undo": "babel-node node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/space-bn-backend.git"
  },
