describe('Post hotel', () => {
  before('clear database', () => {
    db.query('DELETE FROM hotels', (err, results) => {
      if (err) throw err;
      // eslint-disable-next-line no-console
      else console.log('OK!!!!');
    });
  });
  it('should create hotel', (done) => {
    chai.request(app)
      .post('/api/v1/hotels/createHotel')
      .send(data.postHotel)
      .end((err, res) => {
        if (err) throw err;
        res.should.have.status(201);
        done();
      });
  })
 

